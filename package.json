{
	"name": "sortable-tree",
	"version": "0.7.4",
	"description": "A vanilla TypeScript sortable tree",
	"main": "dist/sortable-tree.js",
	"types": "types/index.d.ts",
	"files": [
		"dist/sortable-tree.*",
		"types",
		"src",
		"LICENSE",
		"README.md"
	],
	"scripts": {
		"clean": "rm -rf ./dist",
		"dev": "webpack serve --open --mode development",
		"build": "npm run clean && webpack --mode production",
		"release": "sh bin/release.sh",
		"prepublishOnly": "npm run build"
	},
	"prettier": {
		"trailingComma": "es5"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/marcantondahmen/sortable-tree.git"
	},
	"keywords": [
		"tree",
		"draggable",
		"sortable",
		"sort"
	],
	"author": "Marc Anton Dahmen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/marcantondahmen/sortable-tree/issues"
	},
	"homepage": "https://github.com/marcantondahmen/sortable-tree#readme",
	"devDependencies": {
		"@fontsource-variable/jetbrains-mono": "^5.0.6",
		"css-loader": "^6.7.3",
		"html-webpack-plugin": "^5.5.0",
		"less": "^4.1.3",
		"less-loader": "^11.1.0",
		"mini-css-extract-plugin": "^2.7.2",
		"modern-normalize": "^1.1.0",
		"nanoid": "^4.0.2",
		"string-replace-loader": "^3.1.0",
		"terser-webpack-plugin": "^5.3.6",
		"ts-loader": "^9.4.2",
		"typeface-inter": "^3.18.1",
		"typescript": "^4.9.4",
		"webpack": "^5.89.0",
		"webpack-cli": "^5.0.1",
		"webpack-dev-server": "^4.15.1"
	}
}
